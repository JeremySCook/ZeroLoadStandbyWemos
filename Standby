/*
Standby code with Wemos D1 mini
Used w/ N-channel MOSFET FQP30N06L
By Jeremy S. Cook
*/

void setup() {
  // initialize digital pin LED_BUILTIN as an output.
  pinMode(D6, OUTPUT);
  pinMode(BUILTIN_LED, OUTPUT);
}

void loop() {
  digitalWrite(D6, HIGH);
  digitalWrite(BUILTIN_LED, LOW);
  delay(5000);
  digitalWrite(D6, LOW);
  delay(500);
}
